        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
        <spring>5.0.11.RELEASE</spring>
        <junit>4.11</junit>
        <mybaits>3.5.6</mybaits>
        <mybaits-spring>2.0.0</mybaits-spring>
